    "socket.io": "^4.6.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "stripe": "^14.9.0",
    "redis": "^4.6.11",
    "bull": "^4.12.0",
    "zod": "^3.22.4",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "pino": "^8.17.2",
    "jsonwebtoken": "^9.0.2",
